<?xml version="1.0" encoding="UTF-8" ?>
<definitions
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:flowable="http://flowable.org/bpmn"
  targetNamespace="Examples">

  <process id="multiTaskProcess" name="A Multi Task Process">
    <startEvent id="theStart"/>
    <sequenceFlow id="flow1" sourceRef="theStart" targetRef="theResponse"/>

    <serviceTask id="theResponse" name="The response task"
      flowable:delegateExpression="${triggerableServiceTask}" flowable:async="true" flowable:triggerable="true"/>
    <sequenceFlow  id="flow3" sourceRef="theResponse" targetRef="secondTask"/>

    <userTask id="secondTask" name="second task" flowable:assignee="${person.username}"/>
    <sequenceFlow  id="flow4" sourceRef="secondTask" targetRef="theEnd"/>

    <endEvent id="theEnd"/>
  </process>

</definitions>

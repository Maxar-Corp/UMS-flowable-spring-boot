<?xml version="1.0" encoding="UTF-8" ?>
<definitions
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:flowable="http://flowable.org/bpmn"
  targetNamespace="Examples">

  <process id="multiTaskProcess" name="A Multi Task Process">
    <startEvent id="theStart"/>
    <sequenceFlow id="flow1" sourceRef="theStart" targetRef="service1"/>

    <serviceTask id="service1" name="1st service"
      flowable:delegateExpression="${triggerableServiceTask}" flowable:async="true" flowable:triggerable="true"/>
    <sequenceFlow  id="flow3" sourceRef="service1" targetRef="task1"/>

    <userTask id="task1" name="1st task" flowable:assignee="${person.username}"/>
    <sequenceFlow  id="flow4" sourceRef="task1" targetRef="theEnd"/>

    <endEvent id="theEnd"/>
  </process>

</definitions>

  <process id="UMSHappyPath" name="UMS Simple Happy Path" isExecutable="true">
    <documentation>A simplified version of the UMS happy path, does not include inclusive gates with email notifications</documentation>

    <startEvent id="startEvent" name="Task Form Entered"
      flowable:formFieldValidation="true">
    </startEvent>

    <serviceTask id="leadReceiveTask" name="Task Added"
      flowable:delegateExpression="${leadReceiveServiceTask}">
    </serviceTask>

    <userTask id="leadAssignTask" name="Lead Assigns Task"
      flowable:formFieldValidation="true"
      flowable:assignee="${assigner.username}">
    </userTask>

    <serviceTask id="assigneeReceiveTask" name="Task Sent to Assignee"
      flowable:delegateExpression="${assigneeReceiveServiceTask}">
    </serviceTask>

    <sequenceFlow id="sid-36BFDAB5-C12E-4455-857F-F1314FD314D9"
      sourceRef="startEvent" targetRef="leadReceiveTask">
    </sequenceFlow>
    <sequenceFlow id="sid-3F504A7F-9658-4902-AD2D-24C328AC2415"
      sourceRef="leadReceiveTask" targetRef="leadAssignTask">
    </sequenceFlow>

    <userTask id="assignSetStatusToIP" name="Assignee Set Status to In Progress"
      flowable:formFieldValidation="true"
      flowable:assignee="${assignee.username}">
    </userTask>

    <userTask id="assignSetStatusToTR" name="Assignee Set Status to To Review"
      flowable:formFieldValidation="true"
      flowable:assignee="${assignee.username}">
    </userTask>

    <sequenceFlow id="sid-6525C03D-2457-486F-9F48-DEAD3BAE4E3F"
      sourceRef="assignSetStatusToIP" targetRef="assignSetStatusToTR">
    </sequenceFlow>

    <userTask id="leadSetStatus" name="Lead Set Task to Complete"
      flowable:formFieldValidation="true"
      flowable:assignee="${assigner.username}">
    </userTask>

    <endEvent id="sid-A80ACAC5-5D98-43E1-9733-2BF6892A4D4D" name="Task complete"></endEvent>

    <sequenceFlow id="sid-00A4E781-94DB-43CA-897C-B733F375D4FA"
      sourceRef="leadAssignTask" targetRef="assigneeReceiveTask">
    </sequenceFlow>
    <sequenceFlow id="sid-A4E49B6E-CDC7-4053-B74A-07F24A1D6527"
      sourceRef="assigneeReceiveTask" targetRef="assignSetStatusToIP">
    </sequenceFlow>

    <exclusiveGateway id="status"></exclusiveGateway>

    <sequenceFlow id="sid-D62E377B-D9DF-4DDD-A505-24E3FF73A15E"
      sourceRef="assignSetStatusToTR" targetRef="leadSetStatus">
    </sequenceFlow>
    <sequenceFlow id="sid-08AC6BA7-D5B9-40B6-BC47-507A58E8ACE0"
      sourceRef="leadSetStatus" targetRef="status">
    </sequenceFlow>

    <sequenceFlow id="sid-2739F20F-3522-4C45-BE7A-0E6281BB5FF6"
      sourceRef="status" targetRef="sid-A80ACAC5-5D98-43E1-9733-2BF6892A4D4D">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${status=="complete"}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="sid-DC04B80F-AF6C-4077-B001-DABD90EFCD57"
      sourceRef="status" targetRef="assignSetStatusToTR">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${status=="incomplete"}]]></conditionExpression>
    </sequenceFlow>
  </process>

